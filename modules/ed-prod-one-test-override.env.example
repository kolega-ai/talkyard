# Example environment variables for ed-prod-one-test-override.yml
# Copy this file to .env or set these variables in your environment

# Test credentials - safe for development and testing
E2E_TEST_PASSWORD=public
FORBIDDEN_PASSWORD=public
POSTGRES_PASSWORD=public

# API secrets for testing - use unique values per test environment
TY_SYSMAINT_API_SECRET=test_sysmaint_api_secret
TY_EMAIL_WEBHOOKS_API_SECRET=test_email_webhooks_secret

# Play Framework secret key - generate unique key for each environment
# Use: openssl rand -base64 32
PLAY_SECRET_KEY=public-test-key-not-for-production

# NOTE: These are test-only values. For production environments,
# generate secure random values and manage them through your
# deployment system's secret management.